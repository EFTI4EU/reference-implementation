    </changeSet>
    <changeSet id="modifyDataTypeControl" author="jthueux">
        <modifyDataType  catalogName="efti"
                         columnName="createddate"
                         newDataType="timestamptz"
                         tableName="control"/>
    </changeSet>
    <changeSet id="modifyDataTypeLastModifiedDateControl" author="jthueux">
        <modifyDataType  catalogName="efti"
                         columnName="lastmodifieddate"
                         newDataType="timestamptz"
                         tableName="control"/>
    </changeSet>
</databaseChangeLog>
